{
  "name": "docs",
  "version": "1.0.12",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "npm run use:local && node server.mjs",
    "dev": "npm run use:local && next dev",
    "types:check": "fumadocs-mdx && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "build:prod": "npm run use:npm && ng build --configuration production",
    "deploy": "ng deploy --base-href=/dga-ui/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "use:local": "npm install --no-save file:.. && echo Using local sdga-ui",
    "use:npm": "node -e \"const fs=require('fs');const pkg=JSON.parse(fs.readFileSync('package.json'));const version=pkg.dependencies['sdga-ui'];console.log('Installing sdga-ui@'+version);\" && npm install && echo Using npm sdga-ui"
  },
  "dependencies": {
    "@webcontainer/api": "^1.6.1",
    "fumadocs-core": "16.2.2",
    "fumadocs-mdx": "14.0.4",
    "fumadocs-ui": "16.2.2",
    "lucide-react": "^0.552.0",
    "next": "^16.0.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/mdx": "^2.0.13",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "baseline-browser-mapping": "^2.9.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3"
  }
}
